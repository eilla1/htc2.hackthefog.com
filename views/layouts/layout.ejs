        <script type="text/javascript">
			// Scroll animation initiation
			// document.querySelector('html').classList.remove('no-js');
			if(!window.Cypress) {
				AOS.init({
					mirror: true
				});

				document.addEventListener('aos:in', function(e) {
					console.log('in!', e.detail);
				});
			}

			/* Toggle between showing and hiding the navigation menu links when the user clicks on the hamburger menu / bar icon */
			window.addEventListener('DOMContentLoaded', event => {
				const hamburger = document.getElementById('hamburger-icon');
				const mobile_link_container = document.getElementById('mobile-link-container');
				const mobile_nav_text = document.getElementById('mobile-nav-htc-title');
				hamburger.addEventListener('click', event => {
					event.preventDefault();
					if(!hamburger.classList.contains('active')) hamburger.classList.add('active');
					else hamburger.classList.remove('active');

					if(!mobile_nav_text.classList.contains('mobile-nav-hide-title')) mobile_nav_text.classList.add('mobile-nav-hide-title');
					else mobile_nav_text.classList.remove('mobile-nav-hide-title');

					if(!mobile_link_container.classList.contains('mobile-nav-display')) mobile_link_container.classList.add('mobile-nav-display');
					else mobile_link_container.classList.remove('mobile-nav-display');
					return false;
				});

				for(let num = 1; num <= 5; num++) {
					const link = document.querySelector('#link' + num);
					link.addEventListener('click', () => {
						hamburger.classList.remove('active');
						mobile_nav_text.classList.add('mobile-nav-hide-title');
						mobile_link_container.classList.add('mobile-nav-display');
						return true;
					});
				}
			});
        </script>
